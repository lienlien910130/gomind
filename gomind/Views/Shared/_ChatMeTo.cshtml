
@model IEnumerable<gomind.Models.Chat>


@foreach (var item in Model)
{
  
    <div class="col-sm-4 col-lg-4 col-md-4">
        <div class="thumbnail">
            <img src="http://placehold.it/320x150" alt="">
            <div class="caption text-center">
                <h4>
                    @item.ProductList.prodlist_name
                    @item.UserName
                </h4>
            </div>
        </div>
    </div>
    
}


